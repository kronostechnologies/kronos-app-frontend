    "@babel/runtime": "7.21.0",
    "@equisoft/eslint-config": "3.0.3",
    "@microsoft/eslint-formatter-sarif": "3.0.0",
    "@xmldom/xmldom": "0.8.7",
    "babel-loader": "9.1.2",
    "blob-polyfill": "7.0.20220408",
    "chai": "4.3.7",
    "chai-as-promised": "7.1.1",
    "core-js": "3.30.1",
    "cross-env": "7.0.3",
    "eslint": "8.39.0",
