    "@microsoft/eslint-formatter-sarif": "2.1.7",
    "@xmldom/xmldom": "0.8.2",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.2.5",
    "blob-polyfill": "6.0.20211015",
    "chai": "4.3.6",
