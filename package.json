{
  "name": "kronos-app-frontend",
  "version": "3.0.1",
  "description": "Javascript application framework by Kronos Technologies",
  "main": "js/src/index.js",
  "module": "js/src/index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.js --color -p",
    "build:dev": "cross-env NODE_ENV=development webpack --config webpack.config.js --color -p",
    "build:watch": "cross-env NODE_ENV=development webpack --config webpack.config.js --color --watch -p",
    "test": "cross-env NODE_ENV=test mocha --require ./js/src/testInit.js --slow 100 --recursive ./js/**/*.test.js",
    "test:ci": "yarn test",
    "eslint": "eslint js/src Gruntfile.js webpack.config.js",
    "eslint:ci": "yarn eslint --format junit -o build/tests/eslint/junit.xml"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kronostechnologies/kronos-app-frontend.git"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.1",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-flow-strip-types": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-flow": "^7.9.0",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.10.2",
    "@equisoft/eslint-config": "~2.0.0-alpha.3",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "core-js": "^3.6.5",
    "cross-env": "^7.0.2",
    "eslint": "^7.2.0",
    "eslint-plugin-flowtype": "^5.1.3",
    "fetch-mock": "^5.11.0",
    "flow-bin": "^0.47.0",
    "formdata-polyfill": "^1.0.7",
    "jsx-control-statements": "^3.2.5",
    "matchdep": "^2.0",
    "mocha": "^5.2.0",
    "node-fetch": "^2.0.0-alpha.5",
    "pnp-webpack-plugin": "^1.6.4",
    "regenerator-runtime": "^0.13.5",
    "sinon": "^2.3.2",
    "url-search-params-polyfill": "^8.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "xmldom": "^0.1.27"
  },
  "dependencies": {
    "@sentry/browser": "^4.2.4",
    "es6-error": "^4.0.2",
