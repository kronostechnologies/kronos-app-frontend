  "dependencies": {
    "@sentry/browser": "^6.19.1",
    "es6-error": "^4.1.1",
    "jquery": "^3.4",
    "jquery-param": "^1.1.8",
    "moment": "^2.29.1",
    "numbro": "^2.3.6"
  }
}
