{
  "name": "kronos-app-frontend",
  "version": "4.3.4",
  "description": "Javascript application framework by Kronos Technologies",
  "main": "js/dist/kronos-app-frontend.js",
  "module": "js/src/index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config internals/webpack/webpack.config.js --color -p",
    "build:dev": "cross-env NODE_ENV=development webpack --config internals/webpack/webpack.config.js --color -p",
    "build:watch": "cross-env NODE_ENV=development webpack --config internals/webpack/webpack.config.js --color --watch -p",
    "test": "cross-env NODE_ENV=test mocha --require ./js/src/testInit.js --slow 100 --recursive ./js/**/*.test.js",
    "test:ci": "yarn test",
    "eslint": "eslint js/src internals/webpack/webpack.config.js",
    "eslint:ci": "yarn eslint --format junit -o build/tests/eslint/junit.xml",
    "bump": "node internals/scripts/bump.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kronostechnologies/kronos-app-frontend.git"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.17.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-flow-strip-types": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-flow": "^7.16.7",
    "@babel/register": "^7.17.7",
    "@babel/runtime": "^7.17.8",
    "@equisoft/eslint-config": "~2.0.2",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.4",
    "blob-polyfill": "^6.0.20211015",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "core-js": "^3.21.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-plugin-flowtype": "^5.10.0",
    "eslint-plugin-import": "^2.25.4",
    "fetch-mock": "^9.11.0",
    "flow-bin": "^0.174.1",
    "formdata-polyfill": "^3.0.20",
    "mocha": "^8.4.0",
    "node-fetch": "^2.6.7",
    "pnp-webpack-plugin": "^1.7.0",
    "prompt-confirm": "^2.0.4",
    "regenerator-runtime": "^0.13.9",
    "shelljs": "^0.8.5",
    "sinon": "^13.0.1",
    "url-search-params-polyfill": "^8.1.1",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12",
    "xmldom": "^0.6.0"
