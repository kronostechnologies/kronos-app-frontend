{
  "name": "@equisoft/kronos-app-frontend",
  "version": "0.0.1",
  "description": "Javascript legacy application framework by Equisoft",
  "main": "js/dist/kronos-app-frontend.js",
  "module": "js/src/index.js",
  "files": [
    "js/"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config internals/webpack/webpack.config.js --color",
    "build:dev": "cross-env NODE_ENV=development webpack --config internals/webpack/webpack.config.js --color",
    "build:watch": "cross-env NODE_ENV=development webpack --config internals/webpack/webpack.config.js --color --watch",
    "test": "cross-env NODE_ENV=test mocha --require ./js/src/testInit.js --slow 100 --recursive ./js/**/*.test.js",
    "test:ci": "NODE_ENV=test nyc -r clover --report-dir build/mocha/coverage mocha --require ./js/src/testInit.js --slow 100 --recursive ./js/**/*.test.js --reporter mocha-junit-reporter --reporter-options mochaFile=build/mocha/junit.xml --non-interactive  --no-progress",
    "eslint": "eslint js/src",
    "eslint:ci": "./internals/scripts/eslint-ci.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kronostechnologies/kronos-app-frontend.git"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.21.4",
    "@babel/eslint-parser": "7.21.3",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "7.20.7",
    "@babel/plugin-proposal-optional-chaining": "7.21.0",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-flow-strip-types": "7.21.0",
    "@babel/plugin-transform-runtime": "7.21.4",
